  display: flex;
  align-items: center;
  border-radius: 10px;
  justify-content: center;
}
.container .content .newKnow .knowWei .weiHead view:first-of-type{
  display: block;
  width: 0;
  height: 0;
  /* z-index: 2; */
  border-top: 10rpx solid transparent;
  border-left: 20rpx solid #DAA520;
  border-bottom: 10rpx solid transparent;
  /* margin-left: 20rpx; */
}
.container .content .newKnow .knowWei .weiHead view:last-of-type{
  font-size: 28rpx;
  color: #DAA520;
  margin-left: 10rpx;
