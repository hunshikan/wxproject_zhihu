/* pages/market/market.wxss */
.container{
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}
.container .align{
  position: fixed;
  top: 0;
  width: 100%;
  opacity: 1;
  z-index: 2;
}
.topInput{
  background-color: #1E90FF;
  height: 8vh;
  width: 100%;
  line-height: 8vh;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center; 
}
.topInput .have icon{
  position: absolute;
  left: 40rpx;
  top: 20rpx;
}
.topInput .have{
  display: flex;
  justify-content: space-around; 
  align-items: center; 
  background-color: #fff;
  width: 85%;
  height: 6vh;
  border-radius: 5px;
  line-height: 6vh;
  margin: 1vh auto;
  position: relative;
  text-align: center;
}
.topInput .less{
  width: 10%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.topInput .less image{
  width: 50rpx;
  height: 50rpx;
  margin-right: 15rpx;
}
.topInput .have input{
  /* display: inline-block;       */
  height: 1vh;
  line-height: 1vh;
  width: 70%;
  font-size: 30rpx;
  padding: 25rpx 0 25rpx 20rpx;
  text-align: left;     
}
.topInput .have .bottom{
  width: 25%;
  height: 3vh;
  line-height: 3vh;
  border-left: 1px solid #f2f2f2;  
  text-align: center;
  position: relative;
  display: flex;
  align-items: center;
}
.topInput .have image{
  position: absolute;
  top: -1rpx;
  left: 25rpx;
  width: 40rpx;
  height: 40rpx;
}
.topInput .have text{
  height: 3vh; 
  color: #808080;
  font-size: 30rpx;
  text-align: center;
  padding-left: 90rpx;
}
.container .bot{
  height: 7.5vh;
  background-color: #fff;
  width: 100%;
  position: fixed;
  bottom: 0;
  opacity: 1;
  z-index: 2;
  box-shadow: .1px -.1px 5px .1px #ccc; 
  display: flex;
  flex-direction: row;
  /* justify-content: space-between; */
  align-content: center;
  /* border-top: .1px solid #ccc;    */
}
.container .dis{
  display: none;
  /* transition: all 1s linear; */
}
.container .bot view{
  padding-top: 5rpx;
  width: 20%;
  height: 7.5vh;
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.container .bot view image{
  position: absolute;
  top: 0;
  width: 60rpx;
  height: 60rpx;
}
.container .bot view view{
  padding-top: 50rpx;
  font-size: 25rpx;
  display: flex;
  flex-direction: column;
  width: 100%;
  color: #808080;
}
.container .content{
  background-color: #f2f2f2; 
  width: 100%;
  padding-top: 8vh;
}
.container .content .lbt{
  position: relative;
  width: 100%;
  height: 25vh;
  overflow: hidden;
}
.container .content .lbt .ul view{
  width: 100%;
  height: 25vh;
}
.container .content .lbt .ul image{
  width: 100%;
  height: 25vh; 
  display: block;
}
.ul view {
    position: absolute;
    top: 0;
    left: 100%;
}
.ul view:nth-child(1) {
    animation: roll 9s ease -3s infinite;
}
.ul view:nth-child(2) {
    animation: roll 9s ease 0s infinite;
}
.ul view:nth-child(3) {
    animation: roll 9s ease 3s infinite;
}
/* .ul view:nth-child(4) {
    animation: roll 12s ease 6s infinite;
} */
@keyframes roll {
    6.67%,
    33.3% {
        transform: translateX(-100%);
    }
    40%,
    to {
        transform: translateX(-200%);
    }
}
.container .content .head{
  width: 100%;
  height: 14vh;
  background-color: #fff; 
}
.container .content .head .nuo{
  margin-left: -10rpx;
}
.container .content .head .headCon{
   margin: 0 auto; 
  /* margin-left: -5rpx;   */
  width: 90%;
  display: flex;  
  padding: 30rpx 0;
}
.container .content .head .headCon .headConView{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .content .head .headCon .headConView view:first-of-type{
  display: flex;
  align-items: center;
}
.container .content .head .headCon .headConView view:first-of-type image{
  width: 50rpx;
  height: 50rpx;
}
.container .content .head .headCon .headConView view:first-of-type text{
  font-size: 35rpx;
  font-weight: 900;
  padding-left: 10rpx;
}
.container .content .head .headCon .headConView view:last-of-type{
  font-size: 25rpx;
  color: #808080;
  width: 100%;
  height: 5vh;
  line-height:5vh;
  /* margin-left: 10rpx; */
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .content .head .headCon .headItem  view:last-of-type{
  border-right: .6px solid #ccc; 
}
.container .content .newKnow{
   height: 29vh; 
  background-color: #fff;
  margin-top: 2vh;
  width: 100%;
}
.container .content .newKnow .knowHead{
  width: 95%;
  margin: 0 auto;
  font-size: 35rpx;
  font-weight: 900;
  height: 6vh;
  line-height: 6vh;
  padding-bottom: 5rpx;
}
.container .content .newKnow .knowCon{
  width: 95%;
  border-top: .6px solid #ccc;
  height: 15vh;
  margin: 0 auto;
}
.container .content .newKnow .knowCon .knowItem{
  height: 3vh;
  display: flex;
